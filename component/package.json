{
  "name": "lucky-wheel-component",
  "version": "1.0.1",
  "description": "A lucky wheel component for Vue 3 and React",
  "main": "dist/vue/index.js",
  "module": "dist/vue/index.esm.js",
  "types": "dist/vue/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/vue/index.esm.js",
      "require": "./dist/vue/index.js",
      "types": "./dist/vue/index.d.ts"
    },
    "./vue": {
      "import": "./dist/vue/index.esm.js",
      "require": "./dist/vue/index.js",
      "types": "./dist/vue/index.d.ts"
    },
    "./react": {
      "import": "./dist/react/index.esm.js",
      "require": "./dist/react/index.js",
      "types": "./dist/react/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "README_en.md"
  ],
  "scripts": {
    "build:component": "npx rollup -c rollup.config.js",
    "dev:vue": "vite examples/vue",
    "dev:react": "vite examples/react",
    "prepublishOnly": "npm run build:component"
  },
  "dependencies": {
    "tslib": "^2.8.1",
    "vue": ">=3.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitejs/plugin-react": "^4.0.0",
    "@vitejs/plugin-vue": "4.5.2",
    "@vue/compiler-sfc": "3.5.13",
    "rollup": "^4.9.1",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-vue": "^6.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.0.0",
    "vite": "5.0.10",
    "vue-tsc": "1.8.27"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0",
    "vue": ">=3.0.0"
  },
  "keywords": [
    "lucky-wheel",
    "vue",
    "react",
    "component",
    "typescript"
  ],
  "author": "tinhour",
  "license": "MIT"
}
